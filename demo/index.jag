            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>

        <div class="navbar-header pull-left">
            <a href="#" class="navbar-brand">
                <small>
                    <img src="img/wso2-logo.png" /> API Store
                </small>
            </a>
        </div>

        <div class="navbar-buttons navbar-header pull-right" role="navigation">
            <ul class="nav header-nav-right">
                <li>
                    <a data-toggle="dropdown" href="#" class="dropdown-toggle">
                        <img class="nav-user-photo" src="img/user.png" alt="Chanaka's Photo">
                        <span class="user-info"><small>Welcome,</small> Chanaka</span>
                        <i class="wso2-icon fa fa-caret-down"></i>
                    </a>

                    <ul class="user-menu dropdown-menu-right dropdown-menu dropdown-yellow dropdown-caret dropdown-close">
                        <li>
                            <a href="#"><i class="wso2-icon fa fa-cog"></i>Settings</a>
                        </li>
                        <li>
                            <a href="#"><i class="wso2-icon fa fa-user"></i>Profile</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="#"><i class="wso2-icon fa fa-power-off"></i>Logout</a>
                        </li>
                    </ul>
                </li>


                <li>
                    <a data-toggle="dropdown" class="dropdown-toggle" href="#"><i class="wso2-icon fa fa-cogs"></i></a>
                    <div class="dropdown-menu dropdown-menu-right dropdown-caret dropdown-close wso2-ui-main-settings clearfix" id="wso2-ui-main-settings">
                        <div class="dropdown-header">
                            <i class="wso2-icon fa fa-cogs"></i>
                            UI Settings
                        </div>
                        <div class="dropdown-content">
                            <div class="checkbox">
                                <label><input type="checkbox" value="fixed-container"> Fixed Size</label>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
